{
  "name": "gist-db",
  "version": "0.1.4",
  "author": "Matthew Chase Whittemore <mcwhittemore@gmail.com>",
  "description": "Treat your gist account like a database",
  "contributors": [
    {
      "name": "Matthew Chase Whittemore",
      "email": "mcwhittemore@gmail.com"
    }
  ],
  "keywords": [
    "database",
    "db",
    "gist",
    "github"
  ],
  "main": "./build/gist-db",
  "scripts": {
    "prepublish": "npm run build",
    "prebuild": "rm -rf build && mkdir build",
    "build": "babel gist-db.js -o build/gist-db.js",
    "watch": "babel gist-db.js -w -o build/gist-db.js",
    "test": "standard"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mcwhittemore/gist-db.git"
  },
  "dependencies": {
    "github": "https://github.com/mcwhittemore/node-github/tarball/master",
    "request": "2.14.0",
    "taffydb": "2.7.2"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-eslint": "^6.0.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-2": "^6.5.0",
    "standard": "^7.1.2"
  }
}
